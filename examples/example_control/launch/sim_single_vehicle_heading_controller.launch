
<launch>
    <arg name="debug" default="0" />
    <arg name="editor" default="1" />
<!--    <arg name="scene_name" default="Warehouse/Scenes/DemoScene"/>-->
<!--    <arg name="scene_name" default="FlightGogglesEnvironments/Abandoned_Factory_Morning_No_Gates"/>-->
<!--    <arg name="scene_name" default="Nature/Forest/Demo/Demo"/>-->
    <arg name="scene_name" default="NatureManufacture Assets/Forest Environment Dynamic Nature/Demo Scenes/Forest Demo Scene"/>
<!--    <arg name="scene_name" default="flightmare_backup"/>-->


    <param name="use_sim_time" value="true"/>

    <node name="rpgg_flightmare_Renderer" pkg="rpgq_simulator" type="RPG_FLIGHTMARE.x86_64" unless="$(arg editor)"/>

    <node name="simulator" pkg="example_control" type="single_vehicle_heading_controller" output="screen" launch-prefix="gdb -ex run --args" if="$(arg debug)" >
        <param name="scene_name" value="$(arg scene_name)" />
    </node>
    <node name="simulator" pkg="example_control" type="single_vehicle_heading_controller" output="screen" unless="$(arg debug)">
        <param name="scene_name" value="$(arg scene_name)" />
    </node>

